               "preferred-install": "dist",
               "sort-packages": true,
	       "allow-plugins": {
			"yiisoft/yii2-composer": true,
                        "wikimedia/composer-merge-plugin": true
		}
	},
	 "extra" : {
		"merge-plugin" : {
			"include" : [
				"./protected/modules/*/composer.json"
			],
			"recurse" : true,
			"replace" : false,
			"ignore-duplicates" : true,
			"merge-dev" : true,
			"merge-extra" : false,
			"merge-extra-deep" : false,
			"merge-scripts" : true
		}
	}
